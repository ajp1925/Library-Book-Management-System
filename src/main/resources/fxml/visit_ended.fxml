<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="root" prefHeight="402.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lbms.controllers.guicontrollers.VisitControllers.VisitEndedController">
    <children>
        <FlowPane alignment="CENTER" styleClass="background" stylesheets="@../css/root.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <BorderPane>
                    <bottom>
                        <FlowPane alignment="TOP_CENTER" prefHeight="140.0" prefWidth="326.0" BorderPane.alignment="CENTER">
                            <children>
                                <Button mnemonicParsing="false" onAction="#home" text="Finish" />
                            </children>
                            <padding>
                                <Insets top="20.0" />
                            </padding>
                        </FlowPane>
                    </bottom>
                    <center>
                        <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                            <children>
                                <VBox alignment="CENTER" spacing="30.0">
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Visit Information">
                                            <font>
                                                <Font name="System Bold" size="38.0" />
                                            </font>
                                        </Text>
                                        <GridPane>
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="143.0" minWidth="10.0" prefWidth="102.0" />
                                                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="194.0" minWidth="10.0" prefWidth="192.0" />
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                            </rowConstraints>
                                            <children>
                                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Visitor ID:" />
                                                <Text fx:id="visitorID" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="1" />
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Visit End Time:" GridPane.rowIndex="1" />
                                    <Text fx:id="visitEndTime" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Visit Duration:" GridPane.rowIndex="2" />
                                    <Text fx:id="visitDuration" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                            </children>
                                        </GridPane>
                                    </children>
                                </VBox>
                            </children>
                        </FlowPane>
                    </center>
                    <top>
                        <FlowPane alignment="CENTER" prefHeight="101.0" BorderPane.alignment="CENTER">
                            <children>
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Visit Ended Successfully">
                                    <font>
                                        <Font name="System Bold" size="48.0" />
                                    </font>
                                </Text>
                            </children>
                        </FlowPane>
                    </top>
                </BorderPane>
            </children>
        </FlowPane>
    </children>
</AnchorPane>
