<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="root" prefHeight="402.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lbms.controllers.guicontrollers.RegisterControllers.VisitorRegisteredController">
    <children>
        <FlowPane alignment="CENTER" styleClass="background" stylesheets="@../css/root.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <BorderPane>
                    <bottom>
                        <FlowPane alignment="TOP_CENTER" prefHeight="140.0" prefWidth="326.0" BorderPane.alignment="CENTER">
                            <children>
                                <VBox alignment="CENTER" prefHeight="78.0" prefWidth="305.0" spacing="25.0">
                                    <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Would you like to create a computer account for this visitor?" />
                              <HBox alignment="CENTER" spacing="100.0">
                                 <children>
                                              <Button mnemonicParsing="false" onAction="#no" prefWidth="50.0" text="No" />
                                    <Button mnemonicParsing="false" onAction="#yes" prefWidth="50.0" text="Yes" />
                                 </children>
                              </HBox>
                                    </children>
                                </VBox>
                            </children>
                            <padding>
                                <Insets top="20.0" />
                            </padding>
                        </FlowPane>
                    </bottom>
                    <center>
                        <FlowPane fx:id="centerPane" alignment="CENTER" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                            <children>
                                <HBox>
                                    <children>
                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Visitor ID: ">
                                            <font>
                                                <Font size="38.0" />
                                            </font>
                                        </Text>
                                        <Text fx:id="visitorID" strokeType="OUTSIDE" strokeWidth="0.0">
                                            <font>
                                                <Font size="38.0" />
                                            </font>
                                        </Text>
                                    </children>
                                </HBox>
                            </children>
                        </FlowPane>
                    </center>
                    <top>
                        <FlowPane alignment="CENTER" prefHeight="101.0" BorderPane.alignment="CENTER">
                            <children>
                                <Text fx:id="header" strokeType="OUTSIDE" strokeWidth="0.0" text="Visitor Registered Successfully">
                                    <font>
                                        <Font name="System Bold" size="48.0" />
                                    </font>
                                </Text>
                            </children>
                        </FlowPane>
                    </top>
                </BorderPane>
            </children>
        </FlowPane>
    </children>
</AnchorPane>
